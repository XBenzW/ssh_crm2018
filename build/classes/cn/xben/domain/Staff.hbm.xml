<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.xben.domain">

	<class name="Staff" table="crm_staff">
		<id name="staff_id">
			<generator class="native"></generator>
		</id>
		<property name="staff_name"></property>
		<property name="staff_gender"></property>
		<property name="staff_born"></property>
		<property name="staff_password"></property>
		<property name="staff_phone"></property>
		<property name="staff_mobile"></property>

	</class>


</hibernate-mapping>