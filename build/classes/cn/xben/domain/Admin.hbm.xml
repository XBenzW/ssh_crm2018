<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.xben.domain">

	<class name="Admin" table="crm_admin">
		<id name="adm_id">
			<generator class="native"></generator>
		</id>
		<property name="adm_name"></property>
		<property name="adm_phone"></property>
		<property name="adm_mobile"></property>
		<property name="adm_password"></property>

	</class>


</hibernate-mapping>